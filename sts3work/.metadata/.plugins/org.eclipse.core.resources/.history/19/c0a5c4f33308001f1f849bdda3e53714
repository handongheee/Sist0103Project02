package spring.mvc.quiz;

import org.springframework.web.bind.annotation.GetMapping;

public class QuizController {
	
	// 3개의 매핑주소를 포워드한 주소로 연결되게 만들고 결과물은 해당 포워드된 jsp 파일에서 나타나게 한다.
	@GetMapping ("/happy")
	public String happy() {
		
		return "nice";
	}
	
	@GetMapping ("/hello")
	public String hello() {

		return "hello";
	}
	
	@GetMapping ("nice/like")
	public String nice() {

		return "happy";
	}


}
